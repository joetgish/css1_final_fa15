#include<iostream>

using namespace std;

int main(){
	
	int daysToChristmas = 12;
	
	while (daysToChristmas > 0){

		cout << daysToChristmas << " days to Christmas" << endl;
		daysToChristmas--;
	}
	cout << "Christmas is here!!" << endl;

	return 0;
}

